{
  "$schema": "https://openapi.vercel.sh/microfrontends.json",
  "applications": {
    "teachers": {
      "development": {
        "fallback": "teachers-mocha.vercel.app"
      },
      "production": {
        "fallback": "teachers.vercel.app"
      }
    },
    "teachers-micro": {
      "packageName": "teachers-micro-app",
      "routing": [
        {
          "paths": [
            "-/react/*",
            "/micro/"
          ]
        }
      ],
      "localProxyPort": 3030
    }
  }
}
